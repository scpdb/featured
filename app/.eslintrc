{
  "extends": "airbnb-base",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-undef": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "max-len": 0,
    "prefer-template": 0,
    "strict": 0,
    "import/no-unresolved": 0,
    "import/no-dynamic-require": 0,
    "import/first": 0,
    "import/newline-after-import": 0,
    "global-require": 0,
    "no-console": 0,
    "no-restricted-globals": 0
  }
}
